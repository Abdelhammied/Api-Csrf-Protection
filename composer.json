{
    "name": "abdelhammied/api-csrf-protection",
    "description": "This Package is used to secure apis from any cross sites request forgery while wroking with mobile applications using rsa assemetric key",
    "require": {
        "phpseclib/phpseclib": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Abdelhammied Elsayed",
            "email": "abdelhammied@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ApiCsrfProtection\\": "Src:/"
        }
    },
    "extra": {
        "laravel": {
            "providers": "ApiCsrfProtection\\Providers\\ApiCsrfProtectionServiceProvider"
        }
    }
}
